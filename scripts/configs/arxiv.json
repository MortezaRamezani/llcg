{
    "dataset": "arxiv",
    "num_layers": 3,
    "hidden_size": 256,
    "layer_norm": true,
    "dropout": 0.5,
    "val_patience": 1500,
    "lr": 1e-3,
    
    // Local Sampler
    "num_samplers": 4,
    "sampler": "neighbor",
    "num_neighbors": [10, 10, 10],

    "num_epochs": 100,
    "local_updates": 64,
    "minibatch_size": 256,
    "rho": 1.0,
    "inc_k": false,

    // for Correction
    "server_sampler": "neighbor",
    "server_minibatch_size": 2048,
    "server_num_neighbors": [10,10,10],
    "server_minibatch": "random",
    "server_updates": 1,
    "server_lr": 1e-2,
    "server_start_epoch": 0,

}

